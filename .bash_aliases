alias bashrc="gedit ~/.bashrc"
alias openf="nautilus ."
alias pixhawk_start="sudo chmod 777 /dev/ttyACM0"
alias apmusb="roslaunch mavros apm.launch"
alias basha="gedit ~/.bash_aliases"
alias px4usb="roslaunch mavros px4.launch"
alias ssr="rosservice call /mavros/set_stream_rate '{message_rate: 200, on_off: 1}'"
alias sethome="rostopic pub /mavros/global_position/home mavros_msgs/HomePosition '{geo: {longitude: 120.0, latitude: 23.0}}'"
alias setpoint="rostopic pub /mavros/setpoint_raw/local mavros_msgs/PositionTarget '{position: {x: 10, y: 10, z: 10}}'"
alias local_p="rostopic echo /mavros/global_position/local -c"
alias global_raw="rostopic echo /mavros/gpsstatus/gps1/raw -c"
alias global_gps="rostopic echo /mavros/global_position/global -c"
#alias t1500="rosservice call /mavros/param/set "{'param_id': 'SERVO3_MIN', 'value': 1500}""
